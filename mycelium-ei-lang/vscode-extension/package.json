{
  "name": "mycelium-ei-lang",
  "displayName": "Mycelium-EI Language Support",
  "description": "Language support for Mycelium-EI-Lang - Bio-inspired programming with quantum computing",
  "version": "0.1.0",
  "publisher": "Mycelium-EI-Lang",
  "author": {
    "name": "Michael Benjamin Crowe"
  },
  "license": "MIT",
  "galleryBanner": {
    "color": "#2d4e3f",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Themes"
  ],
  "keywords": [
    "mycelium",
    "bio-inspired",
    "quantum",
    "ecological",
    "programming"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MichaelCrowe11/mycelium-ei-lang"
  },
  "homepage": "https://github.com/MichaelCrowe11/mycelium-ei-lang",
  "bugs": {
    "url": "https://github.com/MichaelCrowe11/mycelium-ei-lang/issues"
  },
  "activationEvents": [
    "onLanguage:mycelium"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "mycelium",
        "aliases": [
          "Mycelium-EI",
          "mycelium"
        ],
        "extensions": [
          ".myc",
          ".mycelium"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mycelium",
        "scopeName": "source.mycelium",
        "path": "./syntaxes/mycelium.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "mycelium",
        "path": "./snippets/mycelium.json"
      }
    ],
    "themes": [
      {
        "label": "Mycelium Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/mycelium-dark-theme.json"
      }
    ],
    "commands": [
      {
        "command": "mycelium.run",
        "title": "Mycelium: Run Current File"
      },
      {
        "command": "mycelium.optimize",
        "title": "Mycelium: Run Bio-Optimization"
      }
    ],
    "keybindings": [
      {
        "command": "mycelium.run",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "resourceExtname == .myc"
      }
    ],
    "configuration": {
      "title": "Mycelium-EI-Lang",
      "properties": {
        "mycelium.pythonPath": {
          "type": "string",
          "default": "python",
          "description": "Path to Python interpreter"
        },
        "mycelium.enableOptimizations": {
          "type": "boolean",
          "default": true,
          "description": "Enable performance optimizations"
        },
        "mycelium.debugMode": {
          "type": "boolean",
          "default": false,
          "description": "Enable debug output"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "typescript": "^4.9.0",
    "vsce": "^2.15.0"
  }
}