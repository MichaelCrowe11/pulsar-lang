{
  "name": "cypher-lang",
  "version": "0.1.0",
  "description": "Secure compute language for cryptography, ZK circuits, and smart contracts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cypher": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "compile:evm": "node dist/cli/index.js compile --target evm",
    "compile:wasm": "node dist/cli/index.js compile --target wasm"
  },
  "keywords": [
    "cryptography",
    "zero-knowledge",
    "smart-contracts",
    "blockchain",
    "EVM",
    "WASM",
    "secure-computation",
    "MPC",
    "ZK-SNARK"
  ],
  "author": "CypherLang Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "circomlib": "^2.0.5",
    "commander": "^11.1.0",
    "snarkjs": "^0.7.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}